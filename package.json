{
  "name": "nba-jam3",
  "version": "0.0.1",
  "description": "GCP Cloud Functions - Serverless Template for NBA JAM3",
  "author": "OpsGuru.io",
  "scripts": {
    "start": "nx serve",
    "build": "nx build",
    "deploy": "serverless deploy",
    "test": "jest --coverage",
    "test:hello": "jest --selectProjects=hello --coverage",
    "test:profile": "jest --selectProjects=profile --coverage",
    "test:libs": "jest --selectProjects=libs",
    "format:check": "prettier -c .",
    "format:fix": "prettier -w .",
    "lint:check": "eslint \"**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "linters": "npm-run-all -p js-lint adviser-dev ls-lint",
    "ls-lint": "node_modules/.bin/ls-lint"
  },
  "private": true,
  "devDependencies": {
    "@ls-lint/ls-lint": "^1.11.2",
    "@nrwl/cli": "15.1.0",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/workspace": "15.1.0",
    "@serverless/compose": "^1.3.0",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.3.1",
    "nx": "15.1.0",
    "prettier": "^2.6.2",
    "serverless-google-cloudfunctions": "^4.6.0",
    "serverless-plugin-typescript": "^2.1.4",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3",
    "winston": "^3.8.2"
  },
  "workspaces": [
    "src/apps/*"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2"
  }
}
